# ----------------------------------------------------------------------
# Create surface using vertices
# ----------------------------------------------------------------------

# Block is 10m x 10m
# 0 m <= x <= 10 m
# 0 m <= y <= 10 m
reset
create vertex 0.0 0.0 0.0
create vertex 0.0 +10.0 0.0
create vertex +10.0 +10.0 0.0
create vertex +10.0 0.0 0.0
create surface vertex 1 2 3 4
delete vertex all
