group "x_neg" add node in curve 1
nodeset 1 x_neg
nodeset 1 name "x_neg"
# ----------------------------------------------------------------------
# Create nodeset for +y edge
# ----------------------------------------------------------------------
group "y_pos" add node in curve 2
nodeset 2 y_pos
nodeset 2 name "y_pos"
# ----------------------------------------------------------------------
